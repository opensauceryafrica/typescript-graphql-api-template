{
    "family": "accountableuserservice",
    "executionRoleArn": "arn:aws:iam::677602393757:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "accountableuserservice",
            "image": "677602393757.dkr.ecr.eu-north-1.amazonaws.com/accountableuserservice:latest",
            "portMappings": [
                {
                    "containerPort": 8002,
                    "hostPort": 8002,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
}
